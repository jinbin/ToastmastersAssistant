<view class="page-body">
  <view class="weui-cells weui-cells_after-title">

    <!-- <navigator url="/pages/manage/manage" wx:if="{{isOwner}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active" open-type="navigate"> 
			      <view class="weui-cell__hd">
			      </view>
			      <view class="weui-cell__bd">管理入口</view>
			      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
			  </navigator> -->

    <block wx:for="{{ components }}" wx:key="">
      <navigator url="{{ item.url }}" class="weui-cell weui-cell_access" hover-class="weui-cell_active" open-type="{{item.isTab ? 'switchTab':'navigate'}}">
        <view class="weui-cell__hd">
          <!-- <image src="../../images/tabBar/info.png" style="margin-right: 5px;vertical-align: middle;width:25px; height: 25px;"></image> -->
        </view>
        <view class="weui-cell__bd">{{ item.title }}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
    </block>

    <navigator open-type="navigate" url="/pages/contact/contact" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <!-- <view class="weui-cell__hd">
      </view> -->
      <view class="weui-cell__bd" style="color:purple">联系作者</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <!-- <view class="weui-cell__hd">
      </view> -->
      <view class="weui-cell__bd" bindtap="saveQRCode" id="images/qrcode.jpg" style="color:Orange">
        获取小程序码
        <!-- <span style="color:red;">*</span> -->
      </view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>

    <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <!-- <view class="weui-cell__hd">
      </view> -->
      <view class="weui-cell__bd" bindtap="saveQRCode" id="images/qrcode_for_account.jpg" style="color:DarkCyan">
        关注公众号
        <!-- <span style="color:red;">*</span> -->
      </view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>

    <navigator target="miniProgram" open-type="navigate" app-id="wx18a2ac992306a5a4" extra-data="{{ extraData }}" path="pages/apps/largess/detail?id=LYFYxTFDv9E%3D" version="release" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <!-- <view class="weui-cell__hd">
      </view> -->
      <view class="weui-cell__bd" style="color:red">打赏作者</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <!-- <view class="weui-cell__hd">
      </view> -->
      <view class="weui-cell__bd" bindtap="linkTA" style="color:ForestGreen">
        关联头马助手
      </view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>

    <!-- <navigator target="miniProgram" open-type="navigate" app-id="{{ appId }}" extra-data="{{ extraData }}" version="release" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell weui-cell_access">
      <view class="weui-cell__hd">
      </view>
      <view class="weui-cell__bd">建议与反馈</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
    </navigator> -->

  </view>
</view>

