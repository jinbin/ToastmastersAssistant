	<view class="page-body">
		<view class="weui-cells weui-cells_after-title">
			<block wx:for="{{ components }}" wx:key="">
          <navigator url="{{ item.url }}" class="weui-cell weui-cell_access" hover-class="weui-cell_active" open-type="{{item.isTab ? 'switchTab':'navigate'}}"> 
			        <view class="weui-cell__hd">
			            <image src="{{ item.icon }}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
			        </view>
			        <view class="weui-cell__bd">{{ item.title }}</view>
			        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
			    </navigator>
		    </block>
        <navigator target="miniProgram" open-type="navigate" app-id="{{ appId }}" extra-data="{{ extraData }}" version="release" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__hd">
			      <image src="../../images/find-selected.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
			    </view>
			    <view class="weui-cell__bd">建议与反馈</view>
			    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
		</view>
  </view>

