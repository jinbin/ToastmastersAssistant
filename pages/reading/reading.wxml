<!--pages/reading/reading.wxml-->

<view class="container1">
  <image bindtap="saveOfficialQRCode" style="width:100%" src="/images/guesslike-min.jpeg" mode="widthFix"></image>
</view>

<view class="cards" id="cards">

  <view wx:for="{{guessYouLike}}">
    <view class="line" style="background-color: #F8F8F8"></view>

    <view class="card">
      <view class="card_text">
        <navigator url="/pages/testdb/testdb?src={{item.src}}">
          {{item.title}}
        </navigator>
      </view>
    </view>
  </view>

  <view class="line" style="background-color: #F8F8F8"></view>

</view>
